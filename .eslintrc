{
  "parser": "babel-eslint",
  "extends": "airbnb-base",

  "rules": {
    "no-restricted-syntax": [0],
    "no-mixed-operators": [0],
    "no-underscore-dangle": [0],
    "arrow-parens": [0],
    "class-methods-use-this": [0],
    "consistent-return": [0],
    "max-len": [0],
    "semi": [0]
  },

  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
}